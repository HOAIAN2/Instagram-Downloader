{
    "name": "Instagram downloader",
    "description": "Download Photos and Videos from Instagram Post, Reels, TV, Stories",
    "version": "2.5.6",
    "manifest_version": 3,
    "icons": {
        "16": "Instagram_Logo.png",
        "32": "Instagram_Logo.png",
        "48": "Instagram_Logo.png",
        "128": "Instagram_Logo.png"
    },
    "host_permissions": [
        "https://www.instagram.com/*"
    ],
    "action": {
        "default_icon": "Instagram_Logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.instagram.com/*"
            ],
            "js": [
                "src/js/main.js",
                "src/js/post.js",
                "src/js/story.js"
            ],
            "css": [
                "src/style/style.css"
            ]
        }
    ]
}